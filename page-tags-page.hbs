{{!< default}}

{{#post}}
{{#foreach tags}}
<article class="tag">
    <header>
		<h1 class="tag-name"><a href="{{url}}">I am a {{{name}}}</a></h1>
	</header>
    {{#if image}}
		<p class="crop"><a href="{{url}}"><img class="header_image" src="{{image}}"></img></a></p>
    {{/if}}
    <section class="post-excerpt">
        {{description}}
    </section>
</article>
{{/foreach}}
{{/post}}