{{!< default}}

<nav class="pagination bounding">
    {{#if prev}}<div class="newer-posts"><a href="{{page_url prev}}">Last</a></div>{{/if}}
    {{#if next}}<div class="older-posts"><a href="{{page_url next}}">Next</a></div>{{/if}}
    <div class="page-number">Page {{page}} of {{pages}}</div>
</nav>